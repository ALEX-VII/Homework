<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.rbac.dao.TUsrMsgboxDao">

    <resultMap type="com.example.rbac.entity.TUsrMsgbox" id="TUsrMsgboxMap">
        <result property="umId" column="um_id" jdbcType="INTEGER"/>
        <result property="umErr" column="um_err" jdbcType="INTEGER"/>
        <result property="umDetail" column="um_detail" jdbcType="VARCHAR"/>
    </resultMap>

    <!--查询单个-->
    <select id="queryById" resultMap="TUsrMsgboxMap">
        select
          um_id, um_err, um_detail
        from t_usr_msgbox
        where um_id = #{umId}
    </select>

    <!--查询指定行数据-->
    <select id="queryAllByLimit" resultMap="TUsrMsgboxMap">
        select
          um_id, um_err, um_detail
        from t_usr_msgbox
        <where>
            <if test="umId != null">
                and um_id = #{umId}
            </if>
            <if test="umErr != null">
                and um_err = #{umErr}
            </if>
            <if test="umDetail != null and umDetail != ''">
                and um_detail = #{umDetail}
            </if>
        </where>
        limit #{pageable.offset}, #{pageable.pageSize}
    </select>

    <!--统计总行数-->
    <select id="count" resultType="java.lang.Long">
        select count(1)
        from t_usr_msgbox
        <where>
            <if test="umId != null">
                and um_id = #{umId}
            </if>
            <if test="umErr != null">
                and um_err = #{umErr}
            </if>
            <if test="umDetail != null and umDetail != ''">
                and um_detail = #{umDetail}
            </if>
        </where>
    </select>
    <select id="getDetailById" resultType="java.lang.String">
        select
            um_detail
        from t_usr_msgbox
        where um_err = #{id}
    </select>

    <!--新增所有列-->
    <insert id="insert" keyProperty="umId" useGeneratedKeys="true">
        insert into t_usr_msgbox(um_err, um_detail)
        values (#{umErr}, #{umDetail})
    </insert>

    <insert id="insertBatch" keyProperty="umId" useGeneratedKeys="true">
        insert into t_usr_msgbox(um_err, um_detail)
        values
        <foreach collection="entities" item="entity" separator=",">
        (#{entity.umErr}, #{entity.umDetail})
        </foreach>
    </insert>

    <insert id="insertOrUpdateBatch" keyProperty="umId" useGeneratedKeys="true">
        insert into t_usr_msgbox(um_err, um_detail)
        values
        <foreach collection="entities" item="entity" separator=",">
            (#{entity.umErr}, #{entity.umDetail})
        </foreach>
        on duplicate key update
        um_err = values(um_err),
        um_detail = values(um_detail)
    </insert>

    <!--通过主键修改数据-->
    <update id="update">
        update t_usr_msgbox
        <set>
            <if test="umErr != null">
                um_err = #{umErr},
            </if>
            <if test="umDetail != null and umDetail != ''">
                um_detail = #{umDetail},
            </if>
        </set>
        where um_id = #{umId}
    </update>

    <!--通过主键删除-->
    <delete id="deleteById">
        delete from t_usr_msgbox where um_id = #{umId}
    </delete>

</mapper>

