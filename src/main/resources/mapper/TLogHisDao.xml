<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.rbac.dao.TLogHisDao">

    <resultMap type="com.example.rbac.entity.TLogHis" id="TLogHisMap">
        <result property="lhId" column="lh_id" jdbcType="INTEGER"/>
        <result property="ihUsrAcc" column="ih_usr_acc" jdbcType="VARCHAR"/>
        <result property="ihDate" column="ih_date" jdbcType="TIMESTAMP"/>
    </resultMap>

    <!--查询单个-->
    <select id="queryById" resultMap="TLogHisMap">
        select
          lh_id, ih_usr_acc, ih_date
        from t_log_his
        where lh_id = #{lhId}
    </select>
    <select id="getAllHis" resultMap="TLogHisMap">
        select
            *
        from t_log_his
    </select>

    <!--查询指定行数据-->
    <select id="queryAllByLimit" resultMap="TLogHisMap">
        select
          lh_id, ih_usr_acc, ih_date
        from t_log_his
        <where>
            <if test="lhId != null">
                and lh_id = #{lhId}
            </if>
            <if test="ihUsrAcc != null and ihUsrAcc != ''">
                and ih_usr_acc = #{ihUsrAcc}
            </if>
            <if test="ihDate != null">
                and ih_date = #{ihDate}
            </if>
        </where>
        limit #{pageable.offset}, #{pageable.pageSize}
    </select>

    <!--统计总行数-->
    <select id="count" resultType="java.lang.Long">
        select count(1)
        from t_log_his
        <where>
            <if test="lhId != null">
                and lh_id = #{lhId}
            </if>
            <if test="ihUsrAcc != null and ihUsrAcc != ''">
                and ih_usr_acc = #{ihUsrAcc}
            </if>
            <if test="ihDate != null">
                and ih_date = #{ihDate}
            </if>
        </where>
    </select>

    <!--新增所有列-->
    <insert id="insert" keyProperty="lhId" useGeneratedKeys="true">
        insert into t_log_his(ih_usr_acc, ih_date)
        values (#{ihUsrAcc}, #{ihDate})
    </insert>

    <insert id="insertRecord" keyProperty="lhId" useGeneratedKeys="false">
        insert into t_log_his(ih_usr_acc)
        values (#{account})
    </insert>

    <insert id="insertBatch" keyProperty="lhId" useGeneratedKeys="true">
        insert into t_log_his(ih_usr_acc, ih_date)
        values
        <foreach collection="entities" item="entity" separator=",">
        (#{entity.ihUsrAcc}, #{entity.ihDate})
        </foreach>
    </insert>

    <insert id="insertOrUpdateBatch" keyProperty="lhId" useGeneratedKeys="true">
        insert into t_log_his(ih_usr_acc, ih_date)
        values
        <foreach collection="entities" item="entity" separator=",">
            (#{entity.ihUsrAcc}, #{entity.ihDate})
        </foreach>
        on duplicate key update
        ih_usr_acc = values(ih_usr_acc),
        ih_date = values(ih_date)
    </insert>

    <!--通过主键修改数据-->
    <update id="update">
        update t_log_his
        <set>
            <if test="ihUsrAcc != null and ihUsrAcc != ''">
                ih_usr_acc = #{ihUsrAcc},
            </if>
            <if test="ihDate != null">
                ih_date = #{ihDate},
            </if>
        </set>
        where lh_id = #{lhId}
    </update>

    <!--通过主键删除-->
    <delete id="deleteById">
        delete from t_log_his where lh_id = #{lhId}
    </delete>

</mapper>

