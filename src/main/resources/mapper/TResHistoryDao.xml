<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.rbac.dao.TResHistoryDao">

    <resultMap type="com.example.rbac.entity.TResHistory" id="TResHistoryMap">
        <result property="reshId" column="resh_id" jdbcType="INTEGER"/>
        <result property="reshUsrAcc" column="resh_usr_acc" jdbcType="VARCHAR"/>
        <result property="reshDate" column="resh_date" jdbcType="TIMESTAMP"/>
        <result property="reshOp" column="resh_op" jdbcType="VARCHAR"/>
    </resultMap>

    <!--查询单个-->
    <select id="queryById" resultMap="TResHistoryMap">
        select
          resh_id, resh_usr_acc, resh_date, resh_op
        from t_res_history
        where resh_id = #{reshId}
    </select>
    <select id="getAllHis" resultMap="TResHistoryMap">
        select
            *
        from t_res_history
    </select>

    <!--查询指定行数据-->
    <select id="queryAllByLimit" resultMap="TResHistoryMap">
        select
          resh_id, resh_usr_acc, resh_date, resh_op
        from t_res_history
        <where>
            <if test="reshId != null">
                and resh_id = #{reshId}
            </if>
            <if test="reshUsrAcc != null and reshUsrAcc != ''">
                and resh_usr_acc = #{reshUsrAcc}
            </if>
            <if test="reshDate != null">
                and resh_date = #{reshDate}
            </if>
            <if test="reshOp != null and reshOp != ''">
                and resh_op = #{reshOp}
            </if>
        </where>
        limit #{pageable.offset}, #{pageable.pageSize}
    </select>

    <!--统计总行数-->
    <select id="count" resultType="java.lang.Long">
        select count(1)
        from t_res_history
        <where>
            <if test="reshId != null">
                and resh_id = #{reshId}
            </if>
            <if test="reshUsrAcc != null and reshUsrAcc != ''">
                and resh_usr_acc = #{reshUsrAcc}
            </if>
            <if test="reshDate != null">
                and resh_date = #{reshDate}
            </if>
            <if test="reshOp != null and reshOp != ''">
                and resh_op = #{reshOp}
            </if>
        </where>
    </select>

    <!--新增所有列-->
    <insert id="insert" keyProperty="reshId" useGeneratedKeys="true">
        insert into t_res_history(resh_usr_acc, resh_date, resh_op)
        values (#{reshUsrAcc}, #{reshDate}, #{reshOp})
    </insert>

    <insert id="addHis" keyProperty="reshId" useGeneratedKeys="false">
        insert into t_res_history(resh_usr_acc,resh_op)
        values (#{account},#{operation})
    </insert>

    <insert id="insertBatch" keyProperty="reshId" useGeneratedKeys="true">
        insert into t_res_history(resh_usr_acc, resh_date, resh_op)
        values
        <foreach collection="entities" item="entity" separator=",">
        (#{entity.reshUsrAcc}, #{entity.reshDate}, #{entity.reshOp})
        </foreach>
    </insert>

    <insert id="insertOrUpdateBatch" keyProperty="reshId" useGeneratedKeys="true">
        insert into t_res_history(resh_usr_acc, resh_date, resh_op)
        values
        <foreach collection="entities" item="entity" separator=",">
            (#{entity.reshUsrAcc}, #{entity.reshDate}, #{entity.reshOp})
        </foreach>
        on duplicate key update
        resh_usr_acc = values(resh_usr_acc),
        resh_date = values(resh_date),
        resh_op = values(resh_op)
    </insert>

    <!--通过主键修改数据-->
    <update id="update">
        update t_res_history
        <set>
            <if test="reshUsrAcc != null and reshUsrAcc != ''">
                resh_usr_acc = #{reshUsrAcc},
            </if>
            <if test="reshDate != null">
                resh_date = #{reshDate},
            </if>
            <if test="reshOp != null and reshOp != ''">
                resh_op = #{reshOp},
            </if>
        </set>
        where resh_id = #{reshId}
    </update>

    <!--通过主键删除-->
    <delete id="deleteById">
        delete from t_res_history where resh_id = #{reshId}
    </delete>

</mapper>

